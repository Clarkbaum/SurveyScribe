{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-console": "off"
  }
}
